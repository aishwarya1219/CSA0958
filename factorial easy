import java.util.*;
public class factorial{
    public static void main(String args[])
    {
        int i,fact=1,number;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter the factorial:");
        number=sc.nextInt();
        for(i=1;i<=number;i++)
        {
            fact=fact*i;
        }
        System.out.println("Factorial of "+number+" is: "+fact);
    }
}
