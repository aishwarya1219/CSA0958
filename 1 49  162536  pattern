import java.util.Scanner;

public class pattern{
    public static void main(String[] args){
        Scanner sc=new Scanner(System.in);
        int a,n=1;
        System.out.println("enter the number: ");
        a=sc.nextInt();
        for(int i=1;i<=a;i++){
            for(int j=1;j<=i;j++){
                System.out.print(" "+n*n);
                n=n+1;


            }
            System.out.print("\n");
        }
    }
}
